package IntPrograms;

public class Patterns {

	public static void main(String[] args) {
		pattern1();
		System.out.println("______________________");
	}

	public static void pattern1() {

//		 * * * *
//		 * * * *
//		 * * * *
//		 * * * *

		for (int i = 1; i <= 4; i++) {
			for (int j = 1; j <= 4; j++) {
				System.out.print(" *");
			}
			System.out.println();
		}

	};
	
//	 * 
//	 * *
//	 * * *
//	 * * * *
	public static void pattern2() {
		
	}

}
